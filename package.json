{
  "name": "nuxt-bootstrap-icons",
  "version": "3.0.0",
  "description": "An easier way of using bootstrap-icons in your Nuxt project!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyedejioyewole/nuxt-bootstrap-icons.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev docs",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs",
    "lint": "eslint . --fix",
    "prepack": "nuxt-module-build build",
    "release": "pnpm lint && changelogen --release --no-output --push",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.6",
    "bootstrap-icons": "^1.13.1",
    "change-case": "^5.4.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.5.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.6",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.0.10",
    "changelogen": "^0.6.1",
    "eslint": "^9.30.1",
    "nuxt": "^3.17.6",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
