{
  "name": "nuxt-bootstrap-icons",
  "version": "2.1.1",
  "description": "An easier way of using bootstrap-icons in your Nuxt project!",
  "repository": "oyedejioyewole/nuxt-bootstrap-icons",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs",
    "docs:build": "npm run build --workspace=docs",
    "docs:dev": "npm run dev --workspace=docs",
    "lint": "eslint .",
    "prepack": "nuxt-module-build build",
    "release": "npm run lint && changelogen --release --output docs/content/release-notes.md && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.2",
    "bootstrap-icons": "^1.11.3",
    "change-case": "^5.4.4",
    "nuxt-svgo-loader": "^0.4.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "2.0.0-beta.3",
    "@nuxt/eslint-config": "^0.7.5",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.2",
    "@types/node": "^22.10.7",
    "changelogen": "^0.5.7",
    "eslint": "^9.18.0",
    "nuxt": "^3.15.2",
    "typescript": "~5.7.3"
  }
}
